<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                      http://maven.apache.org/xsd/settings-1.0.0.xsd">

    <!-- NOTE: GITHUB_USERNAME and GITHUB_TOKEN are set by Jenkinsfile from ereefs-ncanimate2 project. -->
    <servers>
        <!-- Used by Maven (Jenkins) to resolve AIMS-KS dependencies when running tests or building the package -->
        <server>
            <id>github_aimsks</id>
            <username>${GITHUB_USERNAME}</username>
            <password>${GITHUB_TOKEN}</password>
        </server>
        <!-- Used by Maven (Jenkins) to resolve Open-AIMS dependencies when running tests or building the package -->
        <server>
            <id>github_openaims</id>
            <username>${GITHUB_USERNAME}</username>
            <password>${GITHUB_TOKEN}</password>
        </server>
    </servers>
</settings>
